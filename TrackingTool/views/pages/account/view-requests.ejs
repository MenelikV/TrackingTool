<body class="dash">

  <div class="container-fluid">
    <div class="jumbotron jumbostyle">

      <h1 class="display4">Pending Requests</h1>
      <br>

      <table class="table table-striped display" id="example">
        <thead style="background-color: #bfd6f0;">
          <tr>
            <th>Name</th>
            <th width="300">Email</th>
            <th class="text-center">Admin</th>
            <th class="text-center">Basic</th>
          </tr>
        </thead>
        <tbody>

          <% result.forEach(function(item){ %>

          <tr>
            <td>
              <%= item.fullName %>
            </td>

            <td>
              <%= item.emailAddress %> 
            </td>

            <td class="text-center">

              <% if(item.isSuperAdmin) { %>

              <span class="dot"></span>
              <input type="hidden" id="t">

              <%}%>

            </td>

            <td class="text-center">
              <% if(item.isBasicUser) { %>
              <span class="dot"></span>

              <%}%>
            </td>

          </tr>
          <% }) %>
        </tbody>
      </table>

      <button type="submit" class="btn btn-primary ml-auto" style="float: right;" form="uploaded-data">Confirm
        &raquo;</button>

    </div>
  </div>

</body>
